{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "strict": true,
    "sourceMap": false,
    "removeComments": true,
    "noImplicitAny": true,
    "lib": ["es6", "dom"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["google-apps-script"]
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
